{
    "$schema": "https://json.schemastore.org/prettierrc",
    "printWidth": 100,
    "trailingComma": "none",
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true
}
